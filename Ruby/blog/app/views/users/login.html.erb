<h1>Login</h1>
<%= form_for @user, url: attempt_login_users_path do |f| %>
	
	<p>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</p>

	<p>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</p>

	<p>
    	<%= f.submit("Login") %>
  	</p> 	
<% end %>
<p>
	<%= link_to 'SignUp', new_user_path %>
</p>



